<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <services>
        <service id="pre_register_model" class="TooBig\AppBundle\Model\PreRegisterModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="item_model" class="TooBig\AppBundle\Model\ItemModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="auto_subscription_model" class="TooBig\AppBundle\Model\SubscriptionModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="item_subscribtion_model" class="TooBig\AppBundle\Model\ItemSubscribtionModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="item_ratecomment_model" class="TooBig\AppBundle\Model\RateCommentModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="blueimp_model" class="TooBig\AppBundle\Model\BlueimpModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="rubric_model" class="TooBig\AppBundle\Model\RubricModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="model_model" class="TooBig\AppBundle\Model\ModelModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="brand_model" class="TooBig\AppBundle\Model\BrandModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="size_model" class="TooBig\AppBundle\Model\SizeModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="sizetype_model" class="TooBig\AppBundle\Model\SizeTypeModel">
            <call method="setContainer">
                <argument type="service" id="service_container" />
            </call>
        </service>
        <service id="front.item.form" class="TooBig\AppBundle\Form\ItemForm">
            <argument type="service" id="router"></argument>
        </service>
        <service id="front.ratecomment.form" class="TooBig\AppBundle\Form\Type\RateCommentType">
            <argument type="service" id="router"></argument>
        </service>
        <service id="gregwar.captcha.type" class="Gregwar\CaptchaBundle\Type\CaptchaType">
            <argument type="service" id="session"></argument>
        </service>
        <service id="flash_bag" class="TooBig\AppBundle\Service\FlashBag">
            <argument type="service" id="session"></argument>
        </service>
        <service id="soap.smsc" class="TooBig\AppBundle\Service\SoapClientFactory">
            <argument>%soap.smsc.url%</argument>
            <argument>%soap.smsc.options%</argument>
        </service>
    </services>

</container>