<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="TooBig\AppBundle\Entity\SizeCompliance" table="item__size_compliance">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="updatedAt" type="datetime" column="updated_at"/>
    <many-to-one field="size_1" target-entity="TooBig\AppBundle\Entity\Size">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
    <many-to-one field="size_type_1" target-entity="TooBig\AppBundle\Entity\SizeType">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
    <many-to-one field="size_country_1" target-entity="TooBig\AppBundle\Entity\SizeCountry">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
    <many-to-one field="size_2" target-entity="TooBig\AppBundle\Entity\Size">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
    <many-to-one field="size_type_2" target-entity="TooBig\AppBundle\Entity\SizeType">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
    <many-to-one field="size_country_2" target-entity="TooBig\AppBundle\Entity\SizeCountry">
      <cascade><cascade-persist/></cascade>
    </many-to-one>
  </entity>
</doctrine-mapping>
